<?xml version="1.0"?>
<zonal-statistics xmlns="http://www.nfms.unredd.fao.org/zonal-statistics"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.nfms.unredd.fao.org/zonal-statistics ../../../../main/resources/zonal-statistics-configuration.xsd">
	<variable layer="unredd:temporalMosaic">
		<zone-id-field>id</zone-id-field>
		<presentation-data>
			<title>Data evolution</title>
			<date-format>yyyy</date-format>
		</presentation-data>
	</variable>
	<variable layer="unredd:anotherTemporalMosaic">
		<zone-id-field>id</zone-id-field>
		<presentation-data>
			<title>Data evolution</title>
			<date-format>yy</date-format>
		</presentation-data>
	</variable>
</zonal-statistics>